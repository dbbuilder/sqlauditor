{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "Redis": {
    "Enabled": false
  },
  "SqlAnalyzer": {
    "DefaultTimeout": 300,
    "MaxConcurrentAnalyses": 5,
    "EnableCaching": true,
    "CacheDuration": 3600
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:5173",
      "http://localhost:3000",
      "http://localhost:8080"
    ]
  }
}